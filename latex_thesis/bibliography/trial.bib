@article{weisberg_relativistic_2004,
	title = {Relativistic Binary Pulsar B1913+16: Thirty Years of Observations and Analysis},
	url = {http://arxiv.org/abs/astro-ph/0407149},
	shorttitle = {Relativistic Binary Pulsar B1913+16},
	abstract = {We describe results derived from thirty years of observations of {PSR} B1913+16. Together with the Keplerian orbital parameters, measurements of the relativistic periastron advance and a combination of gravitational redshift and time dilation yield the stellar masses with high accuracy. The measured rate of change of orbital period agrees with that expected from the emission of gravitational radiation, according to general relativity, to within about 0.2 percent. Systematic effects depending on the pulsar distance and on poorly known galactic constants now dominate the error budget, so tighter bounds will be difficult to obtain. Geodetic precession of the pulsar spin axis leads to secular changes in pulse shape as the pulsar-observer geometry changes. This effect makes it possible to model the two-dimensional structure of the beam. We find that the beam is elongated in the latitude direction and appears to be pinched in longitude near its center.},
	journaltitle = {{arXiv}:astro-ph/0407149},
	author = {Weisberg, J. M. and Taylor, J. H.},
	urldate = {2018-04-07},
	date = {2004-07-07},
	eprinttype = {arxiv},
	eprint = {astro-ph/0407149},
	keywords = {Astrophysics, General Relativity and Quantum Cosmology},
	annotation = {Comment: To be published in Binary Radio Pulsars, Proc. Aspen Conference, {ASP} Conf. Series, eds. F.A. Rasio \& I.H. Stairs},
	file = {arXiv\:astro-ph/0407149 PDF:/Users/lorenzosperi/Zotero/storage/JR2K64PZ/Weisberg e Taylor - 2004 - Relativistic Binary Pulsar B1913+16 Thirty Years .pdf:application/pdf;arXiv.org Snapshot:/Users/lorenzosperi/Zotero/storage/KXBVJF2W/0407149.html:text/html}
}

@article{taniguchiVariousFeaturesQuasiequilibrium2003,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {gr-qc/0309045},
  title = {Various Features of Quasiequilibrium Sequences of Binary Neutron Stars in General Relativity},
  volume = {68},
  issn = {0556-2821, 1089-4918},
  doi = {10.1103/PhysRevD.68.124025},
  abstract = {Quasiequilibrium sequences of binary neutron stars are numerically calculated in the framework of the Isenberg-Wilson-Mathews (IWM) approximation of general relativity. The results are presented for both rotation states of synchronized spins and irrotational motion, the latter being considered as the realistic one for binary neutron stars just prior to the merger. We assume a polytropic equation of state and compute several evolutionary sequences of binary systems composed of different-mass stars as well as identical-mass stars with adiabatic indices gamma=2.5, 2.25, 2, and 1.8. From our results, we propose as a conjecture that if the turning point of binding energy (and total angular momentum) locating the innermost stable circular orbit (ISCO) is found in Newtonian gravity for some value of the adiabatic index gamma\_0, that of the ADM mass (and total angular momentum) should exist in the IWM approximation of general relativity for the same value of the adiabatic index.},
  number = {12},
  journal = {Physical Review D},
  author = {Taniguchi, Keisuke and Gourgoulhon, Eric},
  month = dec,
  year = {2003},
  keywords = {Astrophysics,General Relativity and Quantum Cosmology},
  file = {/Users/lorenzosperi/Zotero/storage/M95XJPIC/Taniguchi e Gourgoulhon - 2003 - Various features of quasiequilibrium sequences of .pdf;/Users/lorenzosperi/Zotero/storage/IIIFDSPD/0309045.html}
}


