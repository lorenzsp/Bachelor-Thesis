	annotation = {Comment: 12 pages, proceedings of the "Fifth Joint Italian-Pakistani Workshop on Relativistic Astrophysics", 21-23 July 2016, Lecce, Italy},
	file = {arXiv\:1701.04775 PDF:/Users/lorenzosperi/Zotero/storage/F8FATQCH/Congedo - 2017 - Detection principle of gravitational wave detector.pdf:application/pdf;arXiv.org Snapshot:/Users/lorenzosperi/Zotero/storage/TNSFBPI7/1701.html:text/html}
