	issn = {0004-637X, 1538-4357},
	url = {http://arxiv.org/abs/1011.0718},
	doi = {10.1088/0004-637X/722/2/1030},
	abstract = {We present results of more than three decades of timing measurements of the first known binary pulsar, {PSR} B1913+16. Like most other pulsars, its rotational behavior over such long time scales is significantly affected by small-scale irregularities not explicitly accounted for in a deterministic model. Nevertheless, the physically important astrometric, spin, and orbital parameters are well determined and well decoupled from the timing noise. We have determined a significant result for proper motion, \${\textbackslash}mu\_\{{\textbackslash}alpha\} = -1.43{\textbackslash}pm0.13\$, \${\textbackslash}mu\_\{{\textbackslash}delta\}=-0.70{\textbackslash}pm0.13\$ mas yr\${\textasciicircum}\{-1\}\$. The pulsar exhibited a small timing glitch in May 2003, with \$\{{\textbackslash}Delta f\}/f=3.7{\textbackslash}times10{\textasciicircum}\{-11\}\$, and a smaller timing peculiarity in mid-1992. A relativistic solution for orbital parameters yields improved mass estimates for the pulsar and its companion, \$m\_1=1.4398{\textbackslash}pm0.0002 {\textbackslash} M\_\{{\textbackslash}sun\}\$ and \$m\_2=1.3886{\textbackslash}pm0.0002 {\textbackslash} M\_\{{\textbackslash}sun\}\$. The system's orbital period has been decreasing at a rate \$0.997{\textbackslash}pm0.002\$ times that predicted as a result of gravitational radiation damping in general relativity. As we have shown before, this result provides conclusive evidence for the existence of gravitational radiation as predicted by Einstein's theory.},
	pages = {1030--1034},
	number = {2},
	journaltitle = {The Astrophysical Journal},
	author = {Weisberg, Joel M. and Nice, David J. and Taylor, Joseph H.},
	urldate = {2018-04-07},
	date = {2010-10-20},
	eprinttype = {arxiv},
	eprint = {1011.0718},
	keywords = {General Relativity and Quantum Cosmology, Astrophysics - Astrophysics of Galaxies},
	annotation = {Comment: Published in {APJ}, 722, 1030 (2010)},
	file = {arXiv\:1011.0718 PDF:/Users/lorenzosperi/Zotero/storage/PVD5VDR2/Weisberg et al. - 2010 - Timing Measurements of the Relativistic Binary Pul.pdf:application/pdf;arXiv.org Snapshot:/Users/lorenzosperi/Zotero/storage/WJI984JY/1011.html:text/html}
}

@article{weisberg_relativistic_2004,
	title = {Relativistic Binary Pulsar B1913+16: Thirty Years of Observations and Analysis},
	url = {http://arxiv.org/abs/astro-ph/0407149},
	shorttitle = {Relativistic Binary Pulsar B1913+16},
	abstract = {We describe results derived from thirty years of observations of {PSR} B1913+16. Together with the Keplerian orbital parameters, measurements of the relativistic periastron advance and a combination of gravitational redshift and time dilation yield the stellar masses with high accuracy. The measured rate of change of orbital period agrees with that expected from the emission of gravitational radiation, according to general relativity, to within about 0.2 percent. Systematic effects depending on the pulsar distance and on poorly known galactic constants now dominate the error budget, so tighter bounds will be difficult to obtain. Geodetic precession of the pulsar spin axis leads to secular changes in pulse shape as the pulsar-observer geometry changes. This effect makes it possible to model the two-dimensional structure of the beam. We find that the beam is elongated in the latitude direction and appears to be pinched in longitude near its center.},
	journaltitle = {{arXiv}:astro-ph/0407149},
	author = {Weisberg, J. M. and Taylor, J. H.},
	urldate = {2018-04-07},
	date = {2004-07-07},
	eprinttype = {arxiv},
	eprint = {astro-ph/0407149},
	keywords = {Astrophysics, General Relativity and Quantum Cosmology},
	annotation = {Comment: To be published in Binary Radio Pulsars, Proc. Aspen Conference, {ASP} Conf. Series, eds. F.A. Rasio \& I.H. Stairs},
	file = {arXiv\:astro-ph/0407149 PDF:/Users/lorenzosperi/Zotero/storage/JR2K64PZ/Weisberg e Taylor - 2004 - Relativistic Binary Pulsar B1913+16 Thirty Years .pdf:application/pdf;arXiv.org Snapshot:/Users/lorenzosperi/Zotero/storage/KXBVJF2W/0407149.html:text/html}
